    + print_Y0bus(): void
    + print_Ypbus(): void
    + print_Ynbus(): void
    + print_current(): void
    + print_voltages(): void
    + SLG_fault_values(): void
    + LL_fault_values(): void
    + DLG_fault_values(): void
}
